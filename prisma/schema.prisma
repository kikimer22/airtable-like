generator client {
  provider = "prisma-client"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model DataTable {
  id Int @id @default(autoincrement())

  col_s_01 String?
  col_s_02 String?
  col_s_03 String?
  col_s_04 String?
  col_s_05 String?
  col_s_06 String?
  col_s_07 String?
  col_s_08 String?
  col_s_09 String?
  col_s_10 String?
  col_s_11 String?

  col_n_01 Int?
  col_n_02 Int?
  col_n_03 Int?
  col_n_04 Int?
  col_n_05 Int?
  col_n_06 Int?
  col_n_07 Int?
  col_n_08 Int?
  col_n_09 Int?
  col_n_10 Int?
  col_n_11 Int?

  col_b_01 Boolean?
  col_b_02 Boolean?
  col_b_03 Boolean?
  col_b_04 Boolean?
  col_b_05 Boolean?
  col_b_06 Boolean?
  col_b_07 Boolean?
  col_b_08 Boolean?
  col_b_09 Boolean?
  col_b_10 Boolean?
  col_b_11 Boolean?

  @@map("data_table")
}

model NotificationLogTable {
  id         Int      @id @default(autoincrement())
  created_at DateTime @default(now()) @map("created_at")
  table_name String   @map("table_name") @db.Text
  action     String   @db.Text
  payload    Json     @db.JsonB

  @@map("notification_logs_table")
}
